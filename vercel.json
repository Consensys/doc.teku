{
  "cleanUrls": true,
  "redirects": [
    { "source": "/en/latest", "destination": "/", "permanent": true },
    { "source": "/en/latest/", "destination": "/", "permanent": true },
    {
      "source": "/en/latest/:match(.*)",
      "destination": "/:match(.*)",
      "permanent": true
    },
    { "source": "/en/stable", "destination": "/", "permanent": true },
    { "source": "/en/stable/", "destination": "/", "permanent": true },
    {
      "source": "/en/stable/:match(.*)",
      "destination": "/:match(.*)",
      "permanent": true
    },
    {
      "source": "/:path(?!/(?:development|stable)/).*",
      "destination": "/stable/:path*",
      "permanent": true
    }
  ]
}
